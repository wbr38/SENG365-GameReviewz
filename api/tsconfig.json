    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "strict": true,
        "useUnknownInCatchVariables": false,
        "incremental": true,
        "rootDir": "./src/",
        "outDir": "dist",
        "esModuleInterop": true,
        "noImplicitAny": true,
        "resolveJsonModule": true,
        "sourceMap": true
    }
}
